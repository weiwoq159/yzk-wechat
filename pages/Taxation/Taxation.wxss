/* pages/Taxation/Taxation.wxss */
.hr{
    margin-top: 80rpx
}
.pic{
    height:100%;
    background-size: cover;
    padding:30rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    height: 178rpx;
    margin: 0 25rpx 30rpx 25rpx
}
.left{
    font-size: 28rpx;
    color: #fff;
}
.line{
    width: 60rpx;
    height: 6rpx;
    background: #fff;
    border-radius: 6px;
    margin-top: 6px;
}
.right{
    font-size: 24rpx;
    color: #fff;
}
.right text{
    display: block;
    border:1px solid #fff;
    width: 108rpx;
    height: 30rpx;
    text-align: center;
    border-radius: 30rpx;
    line-height: 30rpx;
}